#include "stdafx.h"
#include <iostream>

using namespace std;

int GetRoundToNearest(int originalValue, unsigned int nearestValue);


int _tmain(int argc, _TCHAR* argv[])
{
	int input1, value2;
	cout << "Enter first: ";
	cin >> input1;
	cout << "Enter second: ";
	cin >> value2;

	cout << "Rounded Value = " << GetRoundToNearest(input1,value2) << endl;

	return 0;
}

int GetRoundToNearest(int value1, unsigned int value2){
	if(value2 == 0)
		return 0;
	
	if(value1%value2 >=5){
		value1 = (value1+9)/value2;
		return value1 * value2;
	}
	value1 = value1/value2;
	return value1 = value1 * value2;

};

